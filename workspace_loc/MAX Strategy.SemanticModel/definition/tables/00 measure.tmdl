	measure 'Sales Abs (From 2025)' = ```
			
			VAR Y =
			    SELECTEDVALUE ( dim_period[Year] )
			VAR Base2025 =
			    CALCULATE ( [Sales FCT 2025], ALL ( dim_period ), dim_period[Year] = 2025 )
			RETURN
			SWITCH (
			    TRUE(),
			    ISBLANK ( Y ) || ISBLANK ( Base2025 ) || Y < 2025 || Y > 2030, BLANK(),
			    Y = 2025, Base2025,
			    Base2025 *
			        PRODUCTX (
			            FILTER ( ALL ( dim_period[Year] ), dim_period[Year] >= 2026 && dim_period[Year] <= Y ),
			            1 + COALESCE ( CALCULATE ( [Sales YoY %] ), 0 )
			        )
			)
			
			
			
