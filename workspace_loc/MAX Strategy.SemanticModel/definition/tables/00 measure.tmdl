	measure 'EBIT DIFF CAGR 24-30' =
			
			VAR _start_year = 2024
			VAR _end_year   = 2030
			VAR _start_value =
			    CALCULATE (
			        [EBIT%],
			        ALL ( dim_period[Year] ),
			        dim_period[Year] = _start_year
			    )
			VAR _end_value =
			    CALCULATE (
			        [EBIT%],
			        ALL ( dim_period[Year] ),
			        dim_period[Year] = _end_year
			    )
			RETURN
			    SWITCH (
			        TRUE(),
			        -- case: start < 0 and end > 0 → show as positive improvement
			        _start_value < 0 && _end_value > 0, ABS ( _end_value - _start_value ),
			
			        -- case: normal forward (end year > start year)
			        _end_year > _start_year, _end_value - _start_value,
			
			        -- case: reverse (start year > end year)
			        _start_value - _end_value
			    )
		formatString: 0.0\ %;-0.0\ %;0.0\ %
