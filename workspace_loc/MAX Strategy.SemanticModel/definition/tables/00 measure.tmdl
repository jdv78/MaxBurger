	measure 'EBIT Forecast' =
			
			VAR _actual =
			    DIVIDE(
			        CALCULATE([EBIT], ft_Max[Act_Target] = "Actual"),
			        CALCULATE([Sales], ft_Max[Act_Target] = "Actual")
			    )
			VAR _target =
			    DIVIDE(
			        CALCULATE([EBIT], ft_Max[Act_Target] = "Target"),
			        CALCULATE([Sales], ft_Max[Act_Target] = "Target")
			    )
			VAR _chosen =
			    IF (NOT ISBLANK(_actual) && _actual <> 0, _actual, _target)
			RETURN
			    IF (
			        ISBLANK(_chosen) || _chosen <= -1,
			        BLANK(),
			        _chosen
			    )*[Sales Abs (From 2025)]
