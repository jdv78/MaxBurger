			VAR _end_year = 2024
			VAR _start_value = ABS( CALCULATE( [Sales Act_Target], ALL( dim_period[Year] ), dim_period[Year] = _start_year ) )
			VAR _end_value = ABS(CALCULATE( [Sales Act_Target], ALL( dim_period[Year] ), dim_period[Year] = _end_year ))
